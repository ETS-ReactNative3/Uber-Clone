{
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended"
  ],
   "plugins": ["react", "import", "jsx-a11y"],
  "parser": "@babel/eslint-parser",
  "ecmaFeatures": {
    "classes": true
  },
  "rules": {
    "react/prop-types": 0,
    "linebreak-style": 1,
    "quotes": ["error", "double"],
    "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"] }]
  },
 "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "settings": {
    "import/ignore": ["react-native"],
    "react": {
      "version": "detect"
    }
  }
}